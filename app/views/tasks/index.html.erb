<h1>タスク一覧</h1>
  <p>ログイン中のユーザ：<%= current_user.name %></p>
  <ul>
    <% @tasks.each do |task| %>
     <li><%= link_to task.id, task %> : <%= task.status %> &gt; <%= task.content %></li>
    <% end %>
  </ul>

<%= link_to "新しいタスクの登録", new_task_path %>
<%= link_to "ログアウト", logout_path, method: :delete %>
